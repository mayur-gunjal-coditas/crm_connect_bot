{
    "definition": {
        "$schema": "https://schema.management.azure.com/providers/Microsoft.Logic/schemas/2016-06-01/workflowdefinition.json#",
        "actions": {
            "List_rows_(preview)-copy": {
                "inputs": {
                    "headers": {
                        "accept": "application/json;odata.metadata=full",
                        "organization": "https://stolt-uat.crm4.dynamics.com",
                        "prefer": "odata.include-annotations=*"
                    },
                    "host": {
                        "connection": {
                            "name": "@parameters('$connections')['commondataservice']['connectionId']"
                        }
                    },
                    "method": "get",
                    "path": "/api/data/v9.1/@{encodeURIComponent(encodeURIComponent('leads'))}",
                    "queries": {
                        "$filter": "_createdby_value eq 'f9e26e08-dc74-ef11-a671-000d3ab1415a'  and modifiedon ge @{formatDateTime(addHours(utcNow(), -24), 'yyyy-MM-ddTHH:mm:ss.fffZ')} and statecode ne 2"
                    }
                },
                "runAfter": {},
                "type": "ApiConnection"
            },
            "Update_blob_(V2)-copy-copy": {
                "inputs": {
                    "body": "@body('List_rows_(preview)-copy')?['value']",
                    "headers": {
                        "ReadFileMetadataFromServer": true
                    },
                    "host": {
                        "connection": {
                            "name": "@parameters('$connections')['azureblob-1']['connectionId']"
                        }
                    },
                    "method": "put",
                    "path": "/v2/datasets/@{encodeURIComponent(encodeURIComponent('AccountNameFromSettings'))}/files/@{encodeURIComponent(encodeURIComponent('/container-st-connect-we/st-connect-retrain-data.json'))}"
                },
                "metadata": {
                    "JTJmY29udGFpbmVyLXN0LWNvbm5lY3QtZGV2LXdlJTJmc3QtY29ubmVjdC1yZXRyYWluLWRhdGEuanNvbg==": "/container-st-connect-dev-we/st-connect-retrain-data.json",
                    "JTJmY29udGFpbmVyLXN0LWNvbm5lY3Qtd2UlMmZzdC1jb25uZWN0LXJldHJhaW4tZGF0YS5qc29u": "/container-st-connect-we/st-connect-retrain-data.json"
                },
                "runAfter": {
                    "List_rows_(preview)-copy": [
                        "Succeeded"
                    ]
                },
                "type": "ApiConnection"
            }
        },
        "contentVersion": "1.0.0.0",
        "outputs": {},
        "parameters": {
            "$connections": {
                "defaultValue": {},
                "type": "Object"
            }
        },
        "triggers": {
            "Recurrence": {
                "evaluatedRecurrence": {
                    "frequency": "Day",
                    "interval": 1,
                    "schedule": {
                        "hours": [
                            "20"
                        ],
                        "minutes": [
                            0
                        ]
                    },
                    "startTime": "2024-09-09T23:00:00Z",
                    "timeZone": "UTC"
                },
                "recurrence": {
                    "frequency": "Day",
                    "interval": 1,
                    "schedule": {
                        "hours": [
                            "20"
                        ],
                        "minutes": [
                            0
                        ]
                    },
                    "startTime": "2024-09-09T23:00:00Z",
                    "timeZone": "UTC"
                },
                "type": "Recurrence"
            }
        }
    },
    "parameters": {
        "$connections": {
            "value": {
                "azureblob-1": {
                    "connectionId": "/subscriptions/8ddd0477-caae-4f7e-a4d3-8a9d9b596e7b/resourceGroups/rg-st-jarvis-dev-we/providers/Microsoft.Web/connections/azureblob-1",
                    "connectionName": "azureblob-1",
                    "id": "/subscriptions/a0837f27-f725-4a5d-857a-790b23248145/providers/Microsoft.Web/locations/westeurope/managedApis/azureblob"
                },
                "commondataservice": {
                    "connectionId": "/subscriptions/8ddd0477-caae-4f7e-a4d3-8a9d9b596e7b/resourceGroups/rg-st-jarvis-dev-we/providers/Microsoft.Web/connections/commondataservice",
                    "connectionName": "commondataservice",
                    "id": "/subscriptions/a0837f27-f725-4a5d-857a-790b23248145/providers/Microsoft.Web/locations/westeurope/managedApis/commondataservice"
                }
            }
        }
    }
}